<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!--
            id -> name para reconocer al reporte
            model -> modelo usado para reporte
            string -> titulo para el reporte
            report_type -> qweb-pdf
            name -> nombre del archivo que "dibuja" el reporte
            file -> archivo del reporte
        -->

        <report
            id="action_report_balance_comprobacion"
            model="account.financial.report"
            string="Reporte de balance de comprobación"
            report_type="qweb-pdf"
            name="biosis_cont_report.report_balance_comprobacion"
            file="biosis_cont_report.report_balance_comprobacion"
            />
        <report
            id="action_report_balance_comprobacion_xlsx"
            model="account.financial.report"
            string="Reporte de balance de comprobación"
